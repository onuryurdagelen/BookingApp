<!DOCTYPE html>
<html>
<head>
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
	<title>Ready Bootstrap Dashboard</title>
	<meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0, shrink-to-fit=no' name='viewport' />
	<link rel="stylesheet" href="~/templates/dashboard/assets/css/bootstrap.min.css">
	<link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i">
	<link rel="stylesheet" href="~/templates/dashboard/assets/css/ready.css">
	<link rel="stylesheet" href="~/templates/dashboard/assets/css/demo.css">
</head>
<body>
	<div class="wrapper">
		<!--Main Header starts here-->
		<partial name="~/Areas/Dashboard/Views/Partials/_MainHeaderPartial.cshtml" />
		<!--Main Header ends here-->
		<!--Sidebar starts here-->
		<partial name="~/Areas/Dashboard/Views/Partials/_SidebarPartial.cshtml" />
		<!--Sidebar ends here-->
		<div class="main-panel">
			<div class="content" style="min-height:100vh;">
				<div class="container-fluid">
					@RenderBody()
				</div>
			</div>
		<!--Footer starts here-->
			<partial name="~/Areas/Dashboard/Views/Partials/_FooterPartial.cshtml" />
		<!--Footer ends here-->
		</div>
	</div>
	
</body>
<script src="~/templates/dashboard/assets/js/core/jquery.3.2.1.min.js"></script>
<script src="~/templates/dashboard/assets/js/plugin/jquery-ui-1.12.1.custom/jquery-ui.min.js"></script>
<script src="~/templates/dashboard/assets/js/core/popper.min.js"></script>
<script src="~/templates/dashboard/assets/js/core/bootstrap.min.js"></script>
<script src="~/templates/dashboard/assets/js/plugin/chartist/chartist.min.js"></script>
<script src="~/templates/dashboard/assets/js/plugin/chartist/plugin/chartist-plugin-tooltip.min.js"></script>
@* <script src="~/templates/dashboard/assets/js/plugin/bootstrap-notify/bootstrap-notify.min.js"></script> *@
<script src="~/templates/dashboard/assets/js/plugin/bootstrap-toggle/bootstrap-toggle.min.js"></script>
<script src="~/templates/dashboard/assets/js/plugin/jquery-mapael/jquery.mapael.min.js"></script>
<script src="~/templates/dashboard/assets/js/plugin/jquery-mapael/maps/world_countries.min.js"></script>
<script src="~/templates/dashboard/assets/js/plugin/chart-circle/circles.min.js"></script>
<script src="~/templates/dashboard/assets/js/plugin/jquery-scrollbar/jquery.scrollbar.min.js"></script>
<script src="~/templates/dashboard/assets/js/ready.min.js"></script>
<script src="~/templates/dashboard/assets/js/demo.js"></script>
	
@await RenderSectionAsync("Scripts",false)





</html>